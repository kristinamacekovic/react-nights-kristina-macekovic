{
  "name": "react-nights-kristina-macekovic",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "babel-jest": "^24.7.1",
    "babel-preset-react": "^6.24.1",
    "formik": "^1.5.2",
    "jest": "^24.7.1",
    "jest-styled-components": "^6.3.1",
    "normalize.css": "^8.0.1",
    "qs": "^6.7.0",
    "ramda": "^0.26.1",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-redux": "^6.0.1",
    "react-router": "^5.0.0",
    "react-router-dom": "^5.0.0",
    "react-scripts": "2.1.8",
    "react-test-renderer": "^16.8.6",
    "react-toastify": "^5.1.0",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "styled-components": "^4.2.0",
    "styled-system": "^4.1.0",
    "yup": "^0.27.0"
  },
  "babel": {
    "presets": [
      "react",
      "es2015"
    ]
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "jest",
    "eject": "react-scripts eject",
    "format": "prettier --write '*/**/*.{js,css,md,json}'",
    "lint:js": "eslint . --fix",
    "lint:css": "stylelint 'src/**/*.js'",
    "cypress:open": "cypress open"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!/node_modules/",
      "!src/index.js",
      "!src/serviceWorker.js",
      "!src/store/index.js"
    ]
  },
  "lint-staged": {
    "*.{js}": [
      "eslint --fix",
      "prettier --write",
      "stylelint",
      "git add"
    ],
    "*.{json, md}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@strv/eslint-config-react": "^1.0.1",
    "@strv/stylelint-config-styled-components": "^1.0.0",
    "cypress": "^3.2.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-cypress": "^2.2.1",
    "eslint-plugin-react-hooks": "^1.6.0",
    "fetch-mock": "^7.3.3",
    "husky": ">=1",
    "lint-staged": ">=8",
    "prettier": "^1.16.4",
    "react-testing-library": "^7.0.0",
    "stylelint": "^9.10.1"
  }
}
